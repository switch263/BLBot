# Discord Bot Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# Required: Your Discord bot token from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# Optional: OpenWeather API key (if using weather features)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Optional: Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Optional: Duel cooldown in seconds (default: 60)
DUEL_COOLDOWN=60
